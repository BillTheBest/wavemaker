<div align="center">{+new Link().toFile("index.html").withText("Class Index")+}
| {+new Link().toFile("files.html").withText("File Index")+}</div>
<hr />
<h2>Classes</h2>
<ul class="classList">
	<for each="thisClass" in="data">
		<!--if test="thisClass.comment.getTag('noindex')=='' && thisClass.alias!='_global_'"-->
		<if test="thisClass.comment.getTag('noindex').length==0 && thisClass.alias!='_global_'">
				<li>{!
			output += new Link().toClass(thisClass.alias);
		!}</li>
		</if>
	</for>
</ul>
<hr />
