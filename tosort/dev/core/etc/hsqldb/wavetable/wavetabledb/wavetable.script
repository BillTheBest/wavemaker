CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE CITY(CITY_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR,STATE VARCHAR)
CREATE MEMORY TABLE AREA(AREA_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR,CITY INTEGER,CONSTRAINT SYS_FK_48 FOREIGN KEY(CITY) REFERENCES CITY(CITY_ID))
CREATE MEMORY TABLE CUISINE(CUISINE_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,CUISINE VARCHAR)
CREATE MEMORY TABLE FEATURE(FEATURE_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,FEATURE VARCHAR)
CREATE MEMORY TABLE RESTAURANT(RESTAURANT_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,NAME VARCHAR,ADDRESS VARCHAR,PHONE VARCHAR,SEATS INTEGER,UPDATED DATE,CITY INTEGER,CUISINE INTEGER,AREA INTEGER,FEATURE INTEGER,CONSTRAINT SYS_FK_57 FOREIGN KEY(CITY) REFERENCES CITY(CITY_ID),CONSTRAINT SYS_FK_58 FOREIGN KEY(CUISINE) REFERENCES CUISINE(CUISINE_ID),CONSTRAINT SYS_FK_59 FOREIGN KEY(AREA) REFERENCES AREA(AREA_ID),CONSTRAINT SYS_FK_60 FOREIGN KEY(FEATURE) REFERENCES FEATURE(FEATURE_ID))
CREATE MEMORY TABLE RESERVATION(RESERVATION_ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,RESTAURANT INTEGER,NAME VARCHAR,PHONE VARCHAR,TIME TIME,DATE DATE,PERSONS INTEGER,CONSTRAINT SYS_FK_71 FOREIGN KEY(RESTAURANT) REFERENCES RESTAURANT(RESTAURANT_ID))
ALTER TABLE CITY ALTER COLUMN CITY_ID RESTART WITH 8
ALTER TABLE AREA ALTER COLUMN AREA_ID RESTART WITH 17
ALTER TABLE CUISINE ALTER COLUMN CUISINE_ID RESTART WITH 7
ALTER TABLE FEATURE ALTER COLUMN FEATURE_ID RESTART WITH 5
ALTER TABLE RESTAURANT ALTER COLUMN RESTAURANT_ID RESTART WITH 9
ALTER TABLE RESERVATION ALTER COLUMN RESERVATION_ID RESTART WITH 1
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
