<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<beans xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd" xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <bean lazy-init="true" scope="singleton" class="com.wavemaker.studio.warp.FlowService" id="warpFlowService">
		<property name="warpLoginService">
            <ref bean="warpLoginService"/>
        </property>
		<property name="flowSupport">
            <ref bean="flowSupport"/>
        </property>
	</bean>
    <bean lazy-init="false" scope="singleton" class="com.wavemaker.runtime.service.reflect.ReflectServiceWire">
        <property value="warpFlowService" name="serviceId"/>
        <property ref="JavaService" name="serviceType"/>
    </bean>

	<bean id="flowSupport" scope="singleton" class="com.wavemaker.tools.ws.infoteria.FlowSupport">
        <property name="projectManager">
            <ref bean="projectManager"/>
        </property>
        <property name="designServiceManager">
            <ref bean="designServiceManager"/>
        </property>
    </bean>
</beans>
