<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<beans xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans">
    <bean id="cloudStorageService"
            class="com.wavemaker.studio.CloudStorageService"
            scope="singleton"
            lazy-init="true">
        <property name="cloudStorageMgrBeans">
            <ref bean="cloudStorageMgrBeans"/>
        </property>
		<property name="serviceDeploymentManager">
            <ref bean="serviceDeploymentManager"/>
        </property>
    </bean>

    <bean lazy-init="false"
            scope="singleton"
            class="com.wavemaker.runtime.service.reflect.ReflectServiceWire">
        <property value="cloudStorageService" name="serviceId"/>
        <property name="serviceType" ref="JavaService"/>
    </bean>
</beans>
