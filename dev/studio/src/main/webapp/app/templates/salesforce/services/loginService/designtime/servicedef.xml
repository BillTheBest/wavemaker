<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<service xmlns="http://www.activegrid.com/namespaces/ServiceDefinitions/1.0" springFile="loginService.spring.xml" type="JavaService" CRUDService="false" class="com.sforce.services.LoginService" id="loginService">
    <operation name="logOut">
        <return isList="false" typeRef="java.lang.String"/>
    </operation>
    <operation name="getSessionHeader">
        <return isList="false" typeRef="com.sforce.soap.enterprise.salesforceservice.SessionHeader"/>
    </operation>
    <operation name="logIn">
        <parameter isList="false" typeRef="java.lang.String" name="userName"/>
        <parameter isList="false" typeRef="java.lang.String" name="password"/>
        <return isList="false" typeRef="java.lang.String"/>
    </operation>
    <dataobjects/>
</service>
