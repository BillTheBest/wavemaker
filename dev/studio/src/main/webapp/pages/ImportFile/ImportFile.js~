dojo.provide("wm.studio.pages.ImportProject.ImportProject");

dojo.declare("ImportProject", wm.Page, {
	start: function() {
	},
	cancelButtonClick: function(inSender) {
		wm.fire(this.owner, "dismiss");
	},

      startImportClick: function(inSender) {
	    studio.beginWait("Importing Project...");
	},			  
	importClickCallback: function(inSource, inResponse) {
		studio.endWait();
		var d = studio.startEditor.page.refreshProjectList();
		alert("Successfully imported project " + inResponse);		
		wm.fire(this.owner, "dismiss");
		studio.startEditor.page.openProjectTab();
		studio.startEditor.page.selectProjectInList(inResponse);
	},
	importClickError: function(inSource,inError) {
		studio.endWait();
		alert("Error occurred while importing project!\n" + inError);
	},

	_end: 0
});
