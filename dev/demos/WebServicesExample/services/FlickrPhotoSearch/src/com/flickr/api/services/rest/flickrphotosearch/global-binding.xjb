<?xml version="1.0" encoding="utf-8"?>
<jaxb:bindings
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    jaxb:version="2.0"
    jaxb:extensionBindingPrefixes="xjc">
    <jaxb:globalBindings generateElementProperty="false">
        <xjc:simple/>
        <xjc:javaType name="java.util.Date" xmlType="xs:date" adapter="com.wavemaker.runtime.ws.jaxb.DateXmlAdapter"/>
        <xjc:javaType name="java.util.Date" xmlType="xs:dateTime" adapter="com.wavemaker.runtime.ws.jaxb.DateXmlAdapter"/>
        <xjc:javaType name="java.util.Date" xmlType="xs:time" adapter="com.wavemaker.runtime.ws.jaxb.DateXmlAdapter"/>
    </jaxb:globalBindings>
</jaxb:bindings>