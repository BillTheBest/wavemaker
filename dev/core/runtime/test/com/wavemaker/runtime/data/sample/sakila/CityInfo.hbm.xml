<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.wavemaker.runtime.data.sample.sakila.CityInfo" table="country" catalog="sakila">
        <comment></comment>
        <id name="countryId" type="short" access="field">
            <column name="country_id"/>
            <generator class="assigned" />
        </id>

        <property name="country"/>

        <join table="city">

            <!-- foreign key col -->
            <!--http://www.hibernate.org/hib_docs/v3/reference/en/html_single/#mapping-declaration-key-->
            <key column="country_id"/>

            <property name="city"/>

        </join>

    </class>
</hibernate-mapping>
