<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<service xmlns="http://www.activegrid.com/namespaces/ServiceDefinitions/1.0" 
	springFile="com/wavemaker/runtime/service/waveMakerServiceBean.xml" 
	type="JavaService" CRUDService="false" 
	class="com.wavemaker.runtime.service.WaveMakerService" 
	id="waveMakerService">
  
    <operation name="setTypeManager">
        <parameter isList="false" typeRef="com.wavemaker.runtime.service.TypeManager" name="typeManager"/>
    </operation>
    <operation name="setServiceEventNotifier">
        <parameter isList="false" typeRef="com.wavemaker.runtime.service.events.ServiceEventNotifier" name="serviceEventNotifier"/>
    </operation>
    <operation name="getSessionId">
        <return isList="false" typeRef="java.lang.String"/>
    </operation>
    <operation name="setInternalRuntime">
        <parameter isList="false" typeRef="com.wavemaker.runtime.server.InternalRuntime" name="internalRuntime"/>
    </operation>
    <operation name="echo">
        <parameter isList="false" typeRef="java.lang.String" name="contents"/>
        <parameter isList="false" typeRef="java.lang.String" name="contentType"/>
        <parameter isList="false" typeRef="java.lang.String" name="fileName"/>
        <return isList="false" typeRef="com.wavemaker.runtime.server.DownloadResponse"/>
    </operation>
    <operation name="getInternalRuntime">
        <return isList="false" typeRef="com.wavemaker.runtime.server.InternalRuntime"/>
    </operation>
    <operation name="getTypeManager">
        <return isList="false" typeRef="com.wavemaker.runtime.service.TypeManager"/>
    </operation>
    <operation name="getServiceWire">
        <parameter isList="false" typeRef="java.lang.String" name="serviceName"/>
        <parameter isList="false" typeRef="java.lang.String" name="typeName"/>
        <return isList="false" typeRef="com.wavemaker.runtime.service.ServiceWire"/>
    </operation>
    <operation name="getServiceEventNotifier">
        <return isList="false" typeRef="com.wavemaker.runtime.service.events.ServiceEventNotifier"/>
    </operation>
    <operation name="getLocalHostIP">
        <return isList="false" typeRef="java.lang.String"/>
    </operation>
    <operation name="getRuntimeAccess">
        <return isList="false" typeRef="com.wavemaker.runtime.RuntimeAccess"/>
    </operation>
    <operation name="setRuntimeAccess">
        <parameter isList="false" typeRef="com.wavemaker.runtime.RuntimeAccess" name="runtimeAccess"/>
    </operation>
    <operation name="setServiceManager">
        <parameter isList="false" typeRef="com.wavemaker.runtime.service.ServiceManager" name="serviceManager"/>
    </operation>
    <operation name="getServiceManager">
        <return isList="false" typeRef="com.wavemaker.runtime.service.ServiceManager"/>
    </operation>
</service>
